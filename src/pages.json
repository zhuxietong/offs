{
  "pages": [
    {
      "path": "pages/index/index",
      "style": {
        "navigationBarTitleText": "uni-app",
        "navigationBarBackgroundColor": "#F7F8FA",
        "navigationBarTextStyle": "black",
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/refresh/index",
      "style": {
        "navigationBarTitleText": "uni-app",
        "navigationBarBackgroundColor": "#F7F8FA",
        "navigationBarTextStyle": "black",
        "navigationStyle": "custom"
      }
    }
  ],
  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "uni-app",
    "navigationBarBackgroundColor": "#F7F8FA",
    "backgroundColor": "#F7F8FA",
    "enablePullDownRefresh": false
  },
  "subPackages": [],
  "easycom": {
    "autoscan": true,
    "custom": {
      "me-page-(.*)": "@offs/uni/components/page/$1.vue",
      "me-form-(.*)": "@offs/uni/components/form/$1.vue",
      "me-field-(.*)": "@offs/uni/components/field/$1.vue",
      "me-base-(.*)": "@offs/uni/components/base/$1.vue",
      "me-cmp-(.*)": "@offs/uni/components/cmp/$1.vue",
      "me-icon": "@offs/uni/components/icon/icon.vue",
      "me-(.*)": "@offs/uni/components/$1.vue",
      "^(?!z-paging-refresh|z-paging-load-more)z-paging(.*)": "z-paging/components/z-paging$1/z-paging$1.vue"

    }
  }
}
